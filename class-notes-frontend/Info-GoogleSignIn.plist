<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Google Sign-In Configuration -->
    <!-- Replace these placeholder values with your actual OAuth client IDs -->
    
    <!-- iOS OAuth Client ID from Google Cloud Console -->
    <key>GIDClientID</key>
    <string>YOUR_IOS_CLIENT_ID_HERE</string>
    
    <!-- Server OAuth Client ID for backend authentication -->
    <key>GIDServerClientID</key>
    <string>YOUR_SERVER_CLIENT_ID_HERE</string>
    
    <!-- URL Types for Google Sign-In callback -->
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleURLSchemes</key>
            <array>
                <!-- This should be your reversed client ID -->
                <!-- Example: com.googleusercontent.apps.123456789-abcdefg -->
                <string>YOUR_REVERSED_CLIENT_ID_HERE</string>
            </array>
        </dict>
    </array>
</dict>
</plist> 